<%- include ('vistas') %>
<body>    
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>Tipo</th>
                <th>Dia</th>
                <th>Hora</th>
                <th>Descripcion</th>            
            </tr>
        </thead>
        <tbody>
            <%if (data){%>
                    <% for (i=0;i<data.length;i++){%>
                      <tr>
                        <td> <%=data[i].tipo%></td>
                        <td><%=data[i].dia%></td>        
                        <td><%=data[i].hora%></td>
                        <td><%=data[i].descripcion%></td>
                  </tr>              
                <%}%> 
            <%}%>   
        </tbody>
    </table>
</body>
</html>
